<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <appSettings>
    <!-- 登录时的配置文件选项 -->
    <add key="LogOnTo" value="Config" dispaly="主服务器" />
    <add key="LogOnTo" value="MySqlExample" dispaly="MySql数据库Example" />
    <add key="LogOnTo" value="WebService" dispaly="WebService" />
    <add key="LogOnTo" value="P2PClient02" dispaly="从服务器02" />
    <add key="LogOnTo" value="P2PClient03" dispaly="从服务器03" />
    <add key="DefaultLogOnTo" value="Config" />
    <add key="CurrentUserName" value="Administrator" />
    <add key="CurrentPassword" value="" />
    <add key="RememberPassword" value="True" />
    <add key="RecordLog" value="True" />
    <add key="PersistCookie" value="True" />
    <!-- 客户等相关信息配置 -->
    <add key="CustomerCompanyName" value="bopsoft" />
    <add key="ConfigurationFrom" value="UserConfig" />
    <add key="SoftName" value="CommonTools" />
    <add key="SoftFullName" value="通用快速开发架构" />
    <add key="Version" value="软件版本：YCTrafficSystem v2.3" />
    <add key="RootMenuCode" value="Water" />
    <add key="CurrentLanguage" value="zh-CN" />
    <!-- 用户是否需要配置模块菜单权限 -->
    <add key="UseModulePermission" value="True" />
    <!-- 启用分级管理范围权限设置 -->
    <add key="UsePermissionScope" value="True" />
    <!-- 用户表字段权限的开关 -->
    <add key="UseTableColumnsPermission" value="True" />
    <!-- 登录窗体 -->
    <add key="LogOnAssembly" value="DotNet.WinForm.Utilities" />
    <add key="LogOnForm" value="FrmLogin" />
    <add key="MainForm" value="SDIMainForm" />
    <!-- 默认加载用户 -->
    <add key="LoadUser" value="True" />
    <!-- 允许用户注册 -->
    <add key="AllowUserRegister" value="False" />
    <!-- 不检查在线状态 -->
    <add key="CheckOnLine" value="False" />
    <!-- 加密的方式存储密码 -->
    <add key="EncryptPassword" value="True" />
    <add key="RegisterKey" value="" />
    <!-- Local Remoting WebService -->
    <add key="RunMode" value="Local" />
    <add key="ServiceFactory" value="ServiceFactory" />
    <!-- 数据库相关设定 -->
    <add key="EncryptDbConnection" value="False" />
    <add key="UserCenterDbType" value="SqlServer" />
    <add key="BusinessDbType" value="SqlServer" />
    <add key="WorkFlowDbType" value="SqlServer" />
    <add key="UserCenterDbConnection" value="Data Source=ip;Initial Catalog=yctrafficdb;uid=yctraffic;pwd=;" />
    <add key="BusinessDbConnection" value="Data Source=ip;Initial Catalog=yctrafficdb;uid=yctraffic;pwd=;" />
    <add key="WorkFlowDbConnection" value="Data Source=ip;Initial Catalog=yctrafficdb;uid=yctraffic;pwd=;" />
    <!-- 软件的错误报告发给谁 -->
    <add key="ErrorReportTo" value="test@hotmail.com, test@mail.com" />
    <!-- 发出错误邮箱的地址 -->
    <add key="ErrorReportMailUserName" value="test@gmail.com" />
    <!-- 发出错误邮箱的密码 -->
    <add key="ErrorReportMailPassword" value="123" />

    <!-- 加密的方式存储密码 -->
    <add key="ServerEncryptPassword" value="False" />
  </appSettings>
</configuration>